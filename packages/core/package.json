{
  "name": "viz-ui-y",
  "version": "2.0.0",
  "description": "Components library by Vue3 + Ts",
  "main": "./dist/umd/index.umd.cjs",
  "module": "./dist/es/index.js",
  "types": "./dist/types/core/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/umd/index.umd.cjs",
      "types": "./dist/types/core/index.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "sideEffects": [
    "./dist/index.css",
    "./dist/theme/*.css"
  ],
  "scripts": {
    "build": "run-s clean build-only",
    "build-only": "run-p build-es build-umd",
    "build-umd": "vite build --config build/vite.umd.config.ts",
    "build-es": "vite build --config build/vite.es.config.ts",
    "release": "release-it",
    "clean": "rimraf dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "@popperjs/core": "^2.11.8",
    "async-validator": "^4.2.5"
  },
  "devDependencies": {
    "vite-plugin-dts": "3.9.1",
    "@viz-ui-y/components": "workspace:*",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup-plugin-visualizer": "^5.12.0",
    "vite-plugin-compression": "^0.5.1"
  },
  "peerDependencies": {
    "vue": "^3.4.27"
  }
}
